<table>
    <thead>
        <tr>
            <th>Provider ID</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row =>{ %>
            <tr>
                <td><%= row.provider_id %></td>
                <td><%= row.first_name %></td>
                <td><%= row.last_name %></td>
            </tr>
            <% }); %>
    </tbody>
</table>